<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>8.0</LangVersion>
        <Authors>julesabr</Authors>
        <Description>Shared classes and interfaces that wrap other libraries and APIs</Description>
        <PackageIcon>julesabr.png</PackageIcon>
        <PackageProjectUrl>https://wrappers.julesabr.dev</PackageProjectUrl>
        <RepositoryUrl>https://github.com/julesabr/Wrappers.git</RepositoryUrl>
        <PackageTags>wrapper;wrappers;reflection;julesabr</PackageTags>
        <Copyright>Copyright (c) 2022 Julian Sangillo</Copyright>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2022.1.0" />
    </ItemGroup>

    <ItemGroup>
        <None Include="julesabr.png" Pack="true" PackagePath="" />
        <None Include="LICENSE" Pack="true" PackagePath="" />
    </ItemGroup>

</Project>
